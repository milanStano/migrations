    "type": "project",
    "autoload": {
        "psr-4": {
            "ShopSys\\": [
                "src/ShopSys/"
            ]
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/shopsys/coding-standards.git"
        }
    ],
    "require": {
        "doctrine/dbal": "~2.5",
        "doctrine/doctrine-migrations-bundle": "~1.1",
        "doctrine/orm": "~2.5",
        "jdorn/sql-formatter": "~1.2",
        "php": "^5.6 || ^7.0",
        "symfony/framework-bundle": "~2.7.3 || ~3",
        "symfony/templating": "~2.7.3 || ~3"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.2",
        "shopsys/coding-standards": "2.0.0"
    }
}
